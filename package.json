{
  "name": "casta-fe-playground",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "packages/*",
    "internal/*",
    "examples",
    "docs"
  ],
  "scripts": {
    "cz": "git-cz",
    "gen:cl": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "bootstrap": "pnpm install",
    "dev": "pnpm -C examples dev",
    "build": "gulp -f build/gulpfile.ts",
    "preview": "vite preview",
    "docs:dev": "pnpm -C docs dev",
    "docs:build": "pnpm -C docs build",
    "docs:serve": "pnpm -C docs serve",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.json --max-warnings 0 && pretty-quick --check dev",
    "lint:fix": "eslint --fix . --ext .vue,.js,.ts,.jsx,.tsx,.json",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{packages,docs,examples}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"{packages,docs,examples}/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"{packages,docs,examples}/**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged",
    "reinstall": "rimraf yarn.lock && rimraf pnpm-lock.yaml && rimraf package-lock.json && rimraf node_modules && pnpm bootstrap"
  },
  "dependencies": {
    "@ant-design/colors": "^6.0.0",
    "@ant-design/icons-vue": "^6.0.1",
    "@antv/g6": "4.5.0",
    "@casta-fe-playground/eslint-config": "workspace:*",
    "@casta-fe-playground/stylelint-config": "workspace:*",
    "@casta-fe-playground/enums": "workspace:*",
    "@casta-fe-playground/hooks": "workspace:*",
    "@casta-fe-playground/settings": "workspace:*",
    "@casta-fe-playground/theme-chalk": "workspace:*",
    "@casta-fe-playground/utils": "workspace:*",
    "@casta-fe-playground/components": "workspace:*",
    "@iconify/iconify": "^2.0.4",
    "@vueuse/core": "^6.8.0",
    "@vueuse/shared": "^6.8.0",
    "@zxcvbn-ts/core": "^1.0.0",
    "ant-design-vue": "2.2.8",
    "codemirror": "^5.63.3",
    "cropperjs": "^1.5.12",
    "crypto-js": "^4.1.1",
    "echarts": "^5.2.2",
    "intro.js": "^4.3.0",
    "js-pinyin": "^0.1.9",
    "jsencrypt": "3.2.1",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.2.0",
    "print-js": "^1.6.0",
    "qrcode": "^1.4.4",
    "qs": "^6.10.1",
    "resize-observer-polyfill": "^1.5.1",
    "showdown": "^1.9.1",
    "sortablejs": "^1.14.0",
    "tinymce": "^5.10.1",
    "vditor": "^3.8.7",
    "vue-json-pretty": "^2.0.6",
    "vue-types": "^4.1.1",
    "xlsx": "^0.17.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@iconify/json": "^1.1.426",
    "@purge-icons/generated": "^0.7.0",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.2.0",
    "@types/codemirror": "^5.60.5",
    "@types/crypto-js": "^4.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/gulp-autoprefixer": "^0.0.33",
    "@types/gulp-clean-css": "^4.3.0",
    "@types/gulp-sass": "^5.0.0",
    "@types/inquirer": "^8.1.3",
    "@types/intro.js": "^3.0.2",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^16.11.7",
    "@types/nprogress": "^0.2.0",
    "@types/qrcode": "^1.4.1",
    "@types/qs": "^6.9.7",
    "@types/sass": "^1.43.1",
    "@types/showdown": "^1.9.4",
    "@types/sortablejs": "^1.10.7",
    "@vitejs/plugin-vue": "^2.3.0",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/compiler-sfc": "^3.2.32",
    "chalk": "^5.0.1",
    "clipboard-copy": "^4.0.1",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.2.0",
    "esno": "^0.12.0",
    "fast-glob": "^3.2.11",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-less": "^5.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "11.2.6",
    "postcss": "^8.3.11",
    "postcss-html": "^1.2.0",
    "postcss-less": "^5.0.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.49.11",
    "stylelint": "^14.1.0",
    "sucrase": "^3.20.3",
    "ts-morph": "^14.0.0",
    "typescript": "^4.4.4",
    "vite": "^2.9.0",
    "vue": "^3.2.21",
    "vue-tsc": "^0.29.8"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}